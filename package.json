{
  "name": "app",
  "private": true,
  "version": "0.0.5",
  "description": "Team 04 of NUS ISS MTech SE Bridging Program Project",
  "author": "Star6ucks <foo@bar.baz>",
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "prebuild": "vue-tsc --noEmit --p packages/renderer/tsconfig.json && node scripts/build.mjs",
    "build": "electron-builder",
    "dist": "vue-tsc --noEmit --p packages/renderer/tsconfig.json && node scripts/build.mjs && electron-builder",
    "init": "git config core.hooksPath .git/hooks/ && rm -rf .git/hooks && npx simple-git-hooks",
    "test:e2e": "npx playwright test",
    "test:e2e:headless": "npx playwright test --headed",
    "lint": "eslint --fix packages/**/*.{ts,vue}",
    "format": "prettier -w packages/**/*.{ts,vue} && prettier -w packages/**/*.{ts,vue}",
    "fix": "npm run format && npm run lint"
  },
  "main": "dist/main/index.cjs",
  "engines": {
    "node": ">=14.17.0"
  },
  "nano-staged": {
    "packages/**/*.{ts,vue}": [
      "prettier -w",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@icon-park/vue": "^1.4.0",
    "@playwright/test": "^1.22.2",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vue/composition-api": "^1.6.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "electron": "19.0.1",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-store": "^8.0.1",
    "eslint": "^8.17.0",
    "eslint-plugin-vue": "^9.1.0",
    "nano-staged": "^0.8.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "simple-git-hooks": "^2.8.0",
    "swrv": "^0.9.6",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.3",
    "v8-compile-cache": "^2.3.0",
    "vite": "^2.9.8",
    "vite-plugin-electron": "^0.4.5",
    "vite-plugin-resolve": "^2.1.2",
    "vite-plugin-vue2": "^2.0.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vue-tsc": "^0.35.2",
    "vuex": "^3.4.0"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ]
}
